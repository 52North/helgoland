                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "src/apps/complete/assets",
                "output": "/assets"
              },
              "src/apps/complete/favicon.ico"
            ],
            "styles": [
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "src/apps/timeseries/styles.scss"
            ],
            "scripts": [
              "node_modules/moment/moment.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/plotly.js/dist/plotly-basic.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/apps/timeseries/environments/environment.ts",
                  "with": "src/apps/timeseries/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "timeseries:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "timeseries:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "timeseries:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/apps/timeseries/../../test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/apps/timeseries/polyfills.ts",
            "tsConfig": "src/apps/timeseries/tsconfig.spec.json",
            "scripts": [
              "node_modules/moment/moment.js",
              "node_modules/jquery/dist/jquery.js",
              "node_modules/plotly.js/dist/plotly-basic.js"
            ],
            "styles": [
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.Default.css",
              "node_modules/leaflet.markercluster/dist/MarkerCluster.css",
              "src/apps/timeseries/styles.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "src/apps/complete/assets",
                "output": "/assets"
              },
              "src/apps/complete/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "timeseries-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "timeseries:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "complete",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "complete",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "complete"
    }
  }
}