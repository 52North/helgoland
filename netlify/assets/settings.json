    },
    {
      "name": "lastYear",
      "label": "timeSelection.presets.lastYear",
      "timespan": {
        "from": "moment().subtract(1, 'years').startOf('year')",
        "to":   "moment().subtract(1, 'years').endOf('year')"
      }
    }
  ]
