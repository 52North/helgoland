  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prestart": "node scripts/git.version.js",
    "prebuild": "node scripts/git.version.js",
    "postinstall": "node scripts/adjust-install.js",
    "start": "ng serve timeseries",
    "start:complete": "ng serve complete",
    "prod": "ng serve --prod",
    "build": "ng build timeseries --prod",
    "build:complete": "npm run ng-high-memory -- build complete --prod",
    "prebuild:complete": "npm run prebuild",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e",
    "build-netlify": "npm run prebuild && npm run ng-high-memory && cp ./netlify/* ./dist/complete -r",
    "i18n-extract": "ngx-translate-extract -i ./src -o ./src/assets/i18n/*.json --clean --sort --format namespaced-json --marker marker",
    "bundle-war": "node scripts/bundle-war.js",
    "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "build-distribution-files": "node scripts/build-distributions.js"
  },
  "private": true,
